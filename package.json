{
  "name": "@prefecthq/vue-charts",
  "version": "0.0.12",
  "private": false,
  "keywords": [
    "vue3",
    "charts",
    "d3"
  ],
  "scripts": {
    "serve": "vite",
    "dev": "vue-tsc -w & tsc-alias -w & vite build -w",
    "build": "vue-tsc --declaration --emitDeclarationOnly && vite build",
    "lint": "eslint src && eslint demo",
    "lint:fix": "eslint src --fix && eslint demo --fix",
    "validate:types": "tsc"
  },
  "main": "./dist/vue-charts.umd.js",
  "module": "./dist/vue-charts.es.js",
  "typings": "./dist_types/index.d.ts",
  "files": [
    "src",
    "dist",
    "dist_types"
  ],
  "devDependencies": {
    "@babel/helper-compilation-targets": "^7.16.3",
    "@babel/runtime": "^7.16.3",
    "@prefecthq/eslint-config": "1.0.14",
    "@prefecthq/miter-design": "^0.1.22",
    "@types/d3": "^7.0.0",
    "@types/lodash.debounce": "^4.0.6",
    "@types/node": "^15.14.1",
    "@vitejs/plugin-vue": "^1.2.5",
    "esbuild": "^0.13.4",
    "eslint": "^8.5.0",
    "sass": "^1.35.2",
    "tsc-alias": "^1.6.6",
    "typescript": "^4.1.6",
    "vite": "^2.6.4",
    "vue-router": "^4.0.12",
    "vue-tsc": "^0.33.9"
  },
  "dependencies": {
    "d3": "7.3.0",
    "lodash.debounce": "^4.0.8"
  },
  "peerDependencies": {
    "vue": "^3.0.5"
  }
}