    "@types/node": "18.13.0",
    "@vitejs/plugin-vue": "4.0.0",
    "autoprefixer": "^10.4.13",
    "eslint": "8.34.0",
    "tailwind": "^4.0.0",
    "tsc-alias": "1.8.2",
    "typescript": "4.9.5",
    "vite": "4.1.1",
    "vite-svg-loader": "^4.0.0",
    "vue-router": "^4.1.6",
    "vue-tsc": "1.1.5"
  },
  "dependencies": {
    "@prefecthq/vue-compositions": "1.3.5",
