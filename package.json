{
  "name": "@prefecthq/vue-charts",
  "version": "2.0.0",
  "private": false,
  "keywords": [
    "vue3",
    "charts",
    "d3"
  ],
  "scripts": {
    "serve": "vite --host --mode=demo",
    "dev": "vue-tsc -w & tsc-alias -w & vite build -w",
    "build:demo": "vite build --mode=demo",
    "build": "vue-tsc && tsc-alias && vite build",
    "lint": "eslint src && eslint demo",
    "lint:fix": "eslint src --fix && eslint demo --fix",
    "validate:types": "vue-tsc"
  },
  "main": "./dist/vue-charts.umd.js",
  "module": "./dist/vue-charts.mjs",
  "exports": {
    ".": {
      "import": "./dist/vue-charts.mjs",
      "require": "./dist/vue-charts.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "types": "./dist/types/src/index.d.ts",
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@prefecthq/eslint-config": "1.0.27",
    "@types/d3": "7.4.0",
    "@types/lodash.debounce": "4.0.7",
    "@types/node": "20.5.1",
    "@vitejs/plugin-vue": "4.2.3",
    "autoprefixer": "^10.4.15",
    "eslint": "8.47.0",
    "tailwindcss": "3.3.3",
    "tsc-alias": "1.8.7",
    "typescript": "5.1.6",
    "vite": "4.4.9",
    "vite-svg-loader": "^4.0.0",
    "vue-router": "^4.2.4",
    "vue-tsc": "1.8.8"
  },
  "dependencies": {
    "@prefecthq/vue-compositions": "1.5.7",
    "d3": "7.8.5",
    "date-fns": "^2.30.0",
    "lodash.debounce": "4.0.8"
  },
  "peerDependencies": {
    "@prefecthq/prefect-design": "^2.0.0-beta.3",
    "vue": "^3.2.47"
  }
}
